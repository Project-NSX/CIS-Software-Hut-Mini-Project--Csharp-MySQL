        <Button x:Name="BtnPostcode" Content="Run" HorizontalAlignment="Left" Height="35" Margin="308,333,0,0" VerticalAlignment="Top" Width="129" Click="BtnPostcode_Click"/>
        <DataGrid Name="dataGrid1" ItemsSource="{Binding }" HorizontalAlignment="Left" Height="267" Margin="32,28,0,0" VerticalAlignment="Top" Width="726"/>
        <DataGrid Name="dataGrid2" ItemsSource="{Binding }" HorizontalAlignment="Left" Height="62" VerticalAlignment="Top" Width="315" Margin="458,333,0,0"/>


    </Grid>
</Window>
