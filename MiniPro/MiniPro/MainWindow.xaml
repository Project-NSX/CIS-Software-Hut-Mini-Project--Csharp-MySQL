<Window x:Class="MiniPro.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniPro"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="BtnLaunch" Content="Run" HorizontalAlignment="Left" Height="59" Margin="110,90,0,0" VerticalAlignment="Top" Width="179" Click="BtnLaunch_Click"/>
        <TextBox Name="txtLat1" HorizontalAlignment="Left" Height="33" Margin="128,191,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="152"/>
        <TextBox Name="txtLon1" HorizontalAlignment="Left" Height="33" Margin="375,190,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147"/>
        <TextBox Name="txtLat2" HorizontalAlignment="Left" Height="32" Margin="128,271,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="152"/>
        <TextBox Name="txtLon2" HorizontalAlignment="Left" Height="32" Margin="375,271,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147"/>
        <Label Content="Latitude 1&#xD;&#xA;" HorizontalAlignment="Left" Height="33" Margin="49,191,0,0" VerticalAlignment="Top" Width="65"/>
        <Label Content="Longitude 1" HorizontalAlignment="Left" Height="33" Margin="295,191,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox Name="txtdistance" HorizontalAlignment="Left" Height="25" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" Margin="348,108,0,0"/>
        <Label Content="Radius (in Miles)&#xD;&#xA;" HorizontalAlignment="Left" Margin="425,107,0,0" VerticalAlignment="Top" Height="26"/>
        <Label Content="Latitude 2" HorizontalAlignment="Left" Height="33" Margin="47,271,0,0" VerticalAlignment="Top" Width="67" RenderTransformOrigin="0.489,0.354"/>
        <Label Content="Longitude 2" HorizontalAlignment="Left" Height="33" Margin="295,270,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox Name="postcodeBox" HorizontalAlignment="Left" Height="33" Margin="128,335,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="152"/>
        <Label Content="Postcode (Test)&#xA;" HorizontalAlignment="Left" Height="33" Margin="23,335,0,0" VerticalAlignment="Top" Width="91"/>
        <Button x:Name="BtnPostcode" Content="Run" HorizontalAlignment="Left" Height="35" Margin="308,333,0,0" VerticalAlignment="Top" Width="129" Click="BtnPostcode_Click"/>
        <DataGrid Name="dataTable" HorizontalAlignment="Left" Height="293" Margin="570,90,0,0" VerticalAlignment="Top" Width="188"/>


    </Grid>
</Window>
